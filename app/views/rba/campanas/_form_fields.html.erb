<% f.errores %>
<div class="row">      
  <div class="small-3 columns">
    <%= f.check_box :estado, label: "Activo?" %>
  </div>
  <div class="small-3 columns">
    <%= f.text_field :codigo %>
  </div>
  <div class="small-3 columns">
    <%= f.text_field :nombre %>
  </div>
  <div class="small-3 columns">
    <%= f.text_field :desc %>
  </div>
  <div class="small-3 columns">
    <%= f.collection_select :temporada_id, Rba::Temporada.all, :id, :nombre, {}, {label: "Temporada"} %>
  </div>
  <div class="small-3 columns">
    <%= f.collection_select :campanatipo_id, Rba::Campanatipo.all, :id, :nombre, {}, {label: "tipo"} %>
  </div>
      
</div>