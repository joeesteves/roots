<h1>Usuarios</h1>
<div class="row">
	<div class="small-12 columns">

	<div class="panel">
			<table>
				<thead>
					<tr>
						<th><input type="checkbox" /></th>
						<th>Usuario</th>
						<th>Nombre</th>
						<th>Apellido</th>
						<th>Mail</th>
					</tr>
				</thead>

				<tbody>
				<% @rb_usuarios.each do |rb_usuario| %>
					<tr>
					<td><input type="checkbox" /></td>
					<td><%= rb_usuario.usuario %></td>
					<td><%= rb_usuario.nombre %></td>
					<td><%= rb_usuario.apellido %></td>
					<td><%= rb_usuario.mail %></td>

					<!--	<td><%= link_to 'Show', rb_usuario %></td>
				<td><%= link_to 'Edit', edit_rb_usuario_path(rb_usuario) %></td>
				<td><%= link_to 'Destroy', rb_usuario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
			-->
				</tr>
			<% end %>
				</tbody>
			</table>
		</div>
			<div class="barra_herramientas">
				<%= link_to 'Nuevo', new_rb_usuario_path, :class => "button tiny" %>
	
			</div>
		</div>
		</div>
</div>
<br>
