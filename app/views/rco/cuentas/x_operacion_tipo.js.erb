$('select.debe').empty();
$('select.haber').empty();
<% @ctasDebe.each do |c| %>
$('select.debe').append(new Option('<%= c.nombre%>','<%= c.id %>' ));
<% end %>
$('select.debe').trigger("chosen:updated");
<% @ctasHaber.each do |c| %>
	$('select.haber').append(new Option('<%= c.nombre%>','<%= c.id %>' ));
<% end %>
$('select.haber').trigger("chosen:updated");
