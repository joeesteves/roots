$('#filtroRow').append('<div class="small-7 columns"><select multiple id="cuentas" name="cuentas[]"></select></div>');
<% @rco_cuentas.each do |c| %>

	$('#cuentas').append(new Option('<%= c.nombre%>','<%= c.id %>' ));
<% end %>
<% if @filtro_cuentas_valor %> 
	<% @filtro_cuentas_valor.each do |a| %>
		$('#cuentas option[value=<%=a%>]').prop('selected', true);
	<% end %>
<% end %>
$.fn.initChosen();
$.fn.activarFechaAutocompletado();


