<div class="row">
<div class="small-3 columns">
		<%= f.collection_select :cuenta_id, Rco::Cuenta.all,
		:id, :nombre, {}, {sinEtiqueta: true, class: 'cuenta'} %>
	</div>
	<div class="small-2 columns">
		<%= f.text_field :debe, sinEtiqueta: true, class: 'debe' %>
	</div>
	<div class="small-2 columns">
		<%= f.text_field :haber, sinEtiqueta: true, class: 'haber' %>
	</div>
	<div class="small-4 columns">
		<%= f.text_field :desc, sinEtiqueta: true %>
	</div>
	<div class="small-1 columns">
	<%= f.hidden_field :_destroy %>
	<H4 style='text-align:center'><%= link_to "X", '#', class: "quitar_campos" %></H4>
	 
	</div>

</div>