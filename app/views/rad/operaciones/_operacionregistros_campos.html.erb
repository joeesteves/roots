<div class="row">
<div class="small-3 columns registros">
		<%= f.collection_select :ctaD_id, Rco::Cuenta.includes(:cuentatipo).order('rco_cuentatipos.codigo asc'),
		:id, :nombre, {}, {sinEtiqueta: true, class: 'cuenta'} %>
	</div>
	<div class="small-2 columns registros">
		<%= f.text_field :valor, sinEtiqueta: true, placeholder: 'Valor'  %>
	</div>

	<div class="small-1 columns registros">
		<%= f.hidden_field :_destroy %>
		<h4 style='text-align:center'>
			<%= link_to "O", '#', class: "simil_agrega_campos", remote: true %>
			<%= link_to "X", '', class: "quitar_campos", remote: true %>
		</h4>
	</div>

</div>